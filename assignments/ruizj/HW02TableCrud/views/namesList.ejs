<% - include("template/cabecera", {webTitle: 'Index page' }) %>

        <div class="container">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col"># </th>
                        <th scope="col">Name   </th>
                        <th scope="col">Lastname   </th>
                        <th scope="col">Description   </th>
                        <th scope="col">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (arrayNames.length>0) { %>
                        <% arrayNames.forEach(data=> { %>
                            <tr>
                                <th scope="row">1</th>
                                <td><%= data.name  %> </td>
                                <td><%= data.Lastname %> </td>
                                <td><%= data.description  %> </td>
                                <td> <% data.action %> </td>
                            </tr>
                        <% }) %>
                    <% } %>
                    

                </tbody>
            </table>
        </div>